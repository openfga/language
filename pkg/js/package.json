{
  "name": "@openfga/language",
  "version": "1.0.0",
  "description": "",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build:ts": "rm -rf dist/ && tsc --outDir dist/",
    "build:light": "tsc --outDir dist/",
    "build": "npm run build:ts",
    "build:npx": "tsc --build tsconfig.json",
    "clean": "rm -r dist/",
    "prepublishOnly": "npm run build",
    "test": "jest --config ./jest.config.js",
    "typecheck": "tsc  --skipLibCheck",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format:check": "prettier --check **/*.ts",
    "format:fix": "prettier --write **/*.ts"
  },
  "keywords": [
    "openfga",
    "authorization",
    "fga",
    "fine-grained-authorization",
    "rebac",
    "zanzibar"
  ],
  "author": "OpenFGA",
  "dependencies": {
    "@openfga/sdk": "^0.2.6",
    "antlr4": "^4.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:openfga/language.git"
  },
  "bugs": {
    "url": "https://github.com/openfga/language/issues"
  },
  "homepage": "https://github.com/openfga/language#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
